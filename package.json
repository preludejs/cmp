{
  "name": "@prelude/cmp",
  "version": "1.2.0",
  "description": "Cmp module.",
  "type": "module",
  "module": "index.js",
  "main": "index.cjs",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./*.js": {
      "import": "./*.js",
      "require": "./*.cjs"
    },
    "./*": {
      "require": "./*.cjs"
    }
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tsc && NODE_OPTIONS=--experimental-vm-modules jest",
    "update": "npx npm-check --update --save-exact",
    "preversion": "./preversion.sh",
    "postversion": "git push && git push --tags && npm publish"
  },
  "keywords": [
    "prelude",
    "std",
    "lib",
    "stdlib",
    "library",
    "cmp",
    "compare",
    "sort"
  ],
  "author": "Mirek Rusin (https://github.com/mirek)",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node16": "1.0.2",
    "@types/jest": "27.0.2",
    "eslint": "8.2.0",
    "jest": "27.3.1",
    "typescript": "4.4.4"
  }
}
