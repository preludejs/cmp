{
  "name": "@prelude/cmp",
  "version": "1.3.1",
  "description": "Cmp module.",
  "type": "module",
  "module": "./mjs/index.js",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "import": "./mjs/index.js",
      "require": "./cjs/index.js"
    },
    "./*.js": {
      "import": "./mjs/*.js",
      "require": "./cjs/*.js"
    },
    "./*": {
      "require": "./cjs/*.js"
    }
  },
  "scripts": {
    "test": "tsc && jest",
    "update": "npx npm-check --update --save-exact",
    "preversion": "./preversion.sh",
    "postversion": "git push && git push --tags && npm publish"
  },
  "keywords": [
    "prelude",
    "std",
    "lib",
    "stdlib",
    "library",
    "cmp",
    "compare",
    "sort",
    "order"
  ],
  "author": "Mirek Rusin (https://github.com/mirek)",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node16": "1.0.2",
    "@types/jest": "27.0.2",
    "eslint": "8.2.0",
    "jest": "27.3.1",
    "typescript": "4.4.4"
  }
}
